    <!-- 创建信息 -->
    <insert id="insert" parameterType="WorkType" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO work_type(<include refid="WorkTypeUsedColumns"/>) 
        VALUES 
		(
                 #{id}
				,#{executeTime}
				,#{status}
				,#{createTime}
				,#{updateTime}
				,#{remark}
				,#{yn}
				,#{workId}
