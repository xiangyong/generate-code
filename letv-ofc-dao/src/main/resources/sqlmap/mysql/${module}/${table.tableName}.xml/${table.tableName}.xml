		     </if>
			 <if test="createTime != null and createTime != ''">
                	create_time = #{createTime,jdbcType=TIMESTAMP},
		     </if>
			 <if test="updateTime != null and updateTime != ''">
                	update_time = #{updateTime,jdbcType=TIMESTAMP},
		     </if>
			 <if test="remark != null and remark != ''">
                	remark = #{remark,jdbcType=VARCHAR},
		     </if>
			 <if test="yn != null and yn != ''">
                	yn = #{yn,jdbcType=DECIMAL},
		     </if>
			 <if test="workId != null and workId != ''">
                	work_id = #{workId,jdbcType=DECIMAL},
		     </if>
                where id = #{id}
            </if>
        </if>
    </update>

    <!-- 根据主键获取对象信息 -->
    <select id="getWorkTypeById" resultMap="WorkTypeMap" parameterType="java.lang.Long">
        <if test="_parameter != null">
            select <include refid="WorkTypeColumns"/> from work_type
            where id = #{_parameter} and yn = 1
        </if>
    </select>

  

    <!-- 根据查询Bean获取数据集合，不带翻页 -->
    <select id="queryWorkTypeList" resultMap="WorkTypeMap"
            parameterType="com.letv.ofc.domain.query.WorkTypeQuery">
        select <include refid="WorkTypeColumns"/> from work_type where <include refid="queryWhereWithLike"/>
    </select>

    <!-- 根据查询Bean获取数据集合，翻页 -->
    <select id="queryWorkTypeListWithPage" resultMap="WorkTypeMap"
            parameterType="com.letv.ofc.domain.query.WorkTypeQuery">
        select <include refid="WorkTypeColumns"/> from work_type where <include refid="queryWhereWithLike"/>
        order by create_time desc 
        limit #{startIndex},#{pageSize}
    </select>

    <!-- 根据查询Bean查询-判断是否存在 -->
    <select id="exist" resultType="java.lang.Integer" parameterType="WorkType">
        select count(1) from work_type where <include refid="queryWhereWithLike"/>
    </select>

    <!-- 根据查询Bean获取集合总数 -->
    <select id="queryWorkTypeCount" resultType="java.lang.Integer"
            parameterType="com.letv.ofc.domain.query.WorkTypeQuery">
        select count(1) from work_type where <include refid="queryWhereWithLike"/>
    </select>

   

