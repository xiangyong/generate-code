		     </if>
			 <if test="yn != null and yn != ''">
                	yn = #{yn,jdbcType=DECIMAL},
		     </if>
			 <if test="workId != null and workId != ''">
                	work_id = #{workId,jdbcType=DECIMAL},
		     </if>
			 <if test="id != null and id != ''">
